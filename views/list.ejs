<%- include('header'); -%>
<body>
<div class="box" id="heading">
 <h1 class="heading"> <%= listTitle %> </h1>
</div>
<div class="box">
    <% for (let i = 0 ; i<newitems.length; i++){ %>
 <div class="item">
    <input type="checkbox" >
        <p> <%= newitems[i] %> </p> 
 </div>
 <% } %>  

 <form class = "item" action="/" method="post">
    <input type="text" placeholder="New-todo" name ="newtodo" autocomplete="off">
    <button type="submit"  name = "list" value = "<%= listTitle %>" >+</button>
 </form>
</div>
</body> 
<%- include('footer'); -%>  
</html>